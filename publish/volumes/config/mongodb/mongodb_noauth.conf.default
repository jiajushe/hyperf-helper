#数据库安装位置
dbpath=/server/data/mongodb/databases
#日志存储位置
logpath=/server/data/mongodb/logs/mongodb.log
#是否不覆盖原日志
logappend=true
#是否立即持久化存储
journal=true
#是否以守护进程方式运行
fork=true
#绑定使用IP
bind_ip = 0.0.0.0
#绑定端口默认27017
port = 27017
#是否以安全认证方式运行(即用户登录信息)
noauth = true
quiet=true
enableMajorityReadConcern=true